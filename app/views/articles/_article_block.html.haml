.row-fluid
  %h4
    = link_to article_path(article) do
      %span= article.title
  .author
    %a{:href=>"#"}
      = article.cycle.author.name
  .publishDate
    = article.created_at.localtime
  .description
    - unless article.content.nil?
      = article.content.html_safe
    - else
      = article.tmpContent.html_safe
